    <UseWPF>true</UseWPF>
    <AssemblyName>Microsoft.Msagl.WpfGraphControl</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Condition="  '$(TargetFramework)' == 'net8.0-windows' OR '$(TargetFramework)' == 'net9.0-windows' ">
